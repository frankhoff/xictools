        <td>O</td>              <td>0.0</td>    <td>1.0</td></tr>
    <tr><td><tt>cbd</tt></td>
        <td>zero-bias B-D junction capacitance</td>
        <td>F</td>              <td>0.0</td>    <td>20fF</td></tr>
    <tr><td><tt>cbs</tt></td>
        <td>zero-bias B-S junction capacitance</td>
