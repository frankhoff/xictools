    <tr><td><tt>tm2</tt></td> <td>&nbsp;</td>
        <td>grading coefficient 2nd temp. coeff.</td>
        <td>-</td>      <td>0</td>              <td>&nbsp;</td></tr>
    <tr><td><tt>cjp, cjsw</tt></td> <td>**</td>
        <td>sidewall junction capacitance</td>
        <td>F</td>      <td>0</td>              <td>&nbsp;</td></tr>
